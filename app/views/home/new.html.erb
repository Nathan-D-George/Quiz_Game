<h1> Home </h1>

<% if current_user.present? %>
  <%= link_to "Log Out", destroy_user_session_path, data:{turbo_method: :delete, turbo_confirm: "Log out?"}, class:'btn btn-danger' %>
  <%= link_to "Edit Account", edit_user_registration_path, class:'btn btn-primary' %>
<% else %>
  <%= link_to "Sign Up", new_user_registration_path, class:'btn btn-primary' %>
  <%= link_to "Sign In", new_user_session_path, class:'btn btn-success' %>
<% end %>
<%= fa_icon "gg" %>